<main role="main" class="inner cover">
  <h1 class="cover-heading">France info news</h1>

  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tweet-news">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>

</main>



